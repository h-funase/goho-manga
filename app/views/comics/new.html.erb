<div class="contents row">
  <div class="container">
    <%= form_with(model: @comic, local: true) do |form| %>
      <h3>
        投稿する
      </h3>
      <%= form.text_field :title, placeholder: "タイトル" %>
      <%= form.text_field :image, placeholder: "画像のURL" %>
      <%= form.text_field :url, placeholder: "漫画のURL" %>
      <%= form.text_area :text, placeholder: "text" , rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>